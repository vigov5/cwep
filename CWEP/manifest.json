{
  "name": "Chatwork Enhancement Toolkit",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Add Github @mention to Chatwork. feedback to nguyen.anh.tien@framgia.com",
  "content_scripts": [
    {
      // Change 'matches' attribute to load content
      // script only in pages you want to.
      "matches": ["https://www.chatwork.com/*", "https://kcw.kddi.ne.jp/*"],
      "js": ["jquery-1.11.1.min.js", "contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["main.js", "fuse.min.js", "caretposition.js"],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
